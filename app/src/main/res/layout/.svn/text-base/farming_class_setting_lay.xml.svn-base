<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <include layout="@layout/activity_back_title" />

    <!-- 农事分类设置标题 -->

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/green2"
        android:orientation="horizontal">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="30dip"
            android:gravity="center"
            android:text="农事分类设置"
            android:textColor="@color/white" />
    </LinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="30dip"
        android:gravity="center"
        android:text="操作说明：对农事分类进行编辑并排序"
        android:textColor="@color/black"
        android:textSize="@dimen/text_size_12" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_margin="@dimen/text_size_8"
            android:layout_weight="4"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/shape_green_rec"
                    android:paddingBottom="3sp"
                    android:paddingLeft="10sp"
                    android:paddingTop="3sp"
                    android:text="分类列表"
                    android:textSize="@dimen/text_size_12" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/shape_green_rec"
                    android:orientation="vertical"
                    android:padding="8sp">

                    <ListView
                        android:id="@+id/sort_list"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:visibility="gone"></ListView>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingBottom="3sp"
                        android:paddingLeft="10sp"
                        android:text="采购"
                        android:textSize="@dimen/text_size_12" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:paddingBottom="3sp"
                            android:paddingLeft="30sp"
                            android:text="种苗采购"
                            android:textSize="@dimen/text_size_12" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:paddingBottom="3sp"
                            android:paddingLeft="30sp"
                            android:text="药品采购"
                            android:textSize="@dimen/text_size_12" />
                    </LinearLayout>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingBottom="3sp"
                        android:paddingLeft="10sp"
                        android:text="种植"
                        android:textSize="@dimen/text_size_12" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:paddingBottom="3sp"
                            android:paddingLeft="30sp"
                            android:text="育苗"
                            android:textSize="@dimen/text_size_12" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:paddingBottom="3sp"
                            android:paddingLeft="30sp"
                            android:text="播种"
                            android:textSize="@dimen/text_size_12" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:paddingBottom="3sp"
                            android:paddingLeft="30sp"
                            android:text="除草"
                            android:textSize="@dimen/text_size_12" />
                    </LinearLayout>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingBottom="3sp"
                        android:paddingLeft="10sp"
                        android:text="采摘"
                        android:textSize="@dimen/text_size_12" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:paddingBottom="3sp"
                            android:paddingLeft="30sp"
                            android:text="田间采摘"
                            android:textSize="@dimen/text_size_12" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:paddingBottom="3sp"
                            android:paddingLeft="30sp"
                            android:text="装框"
                            android:textSize="@dimen/text_size_12" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:paddingBottom="3sp"
                            android:paddingLeft="30sp"
                            android:text="上车"
                            android:textSize="@dimen/text_size_12" />
                    </LinearLayout>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingBottom="3sp"
                        android:paddingLeft="10sp"
                        android:text="物流"
                        android:textSize="@dimen/text_size_12" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:paddingBottom="3sp"
                            android:paddingLeft="30sp"
                            android:text="送到收集站"
                            android:textSize="@dimen/text_size_12" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:paddingBottom="3sp"
                            android:paddingLeft="30sp"
                            android:text="物流寄送"
                            android:textSize="@dimen/text_size_12" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/shape_green_rec"
                    android:paddingBottom="3sp"
                    android:paddingLeft="10sp"
                    android:paddingTop="3sp"
                    android:text="操作"
                    android:textSize="@dimen/text_size_12" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/shape_green_rec"
                    android:orientation="vertical"
                    android:padding="8sp">

                    <Button
                        android:id="@+id/btn_top"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/btn_sort_h"
                        android:background="@drawable/btn_bg"
                        android:text="@string/btn_top"
                        android:textColor="@color/white"
                        android:textSize="@dimen/historyscore_tb" />

                    <Button
                        android:id="@+id/btn_up"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/btn_sort_h"
                        android:layout_marginTop="10sp"
                        android:background="@drawable/btn_bg"
                        android:text="@string/btn_up"
                        android:textColor="@color/white"
                        android:textSize="@dimen/historyscore_tb" />

                    <Button
                        android:id="@+id/btn_down"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/btn_sort_h"
                        android:layout_marginTop="10sp"
                        android:background="@drawable/btn_bg"
                        android:text="@string/btn_down"
                        android:textColor="@color/white"
                        android:textSize="@dimen/historyscore_tb" />

                    <Button
                        android:id="@+id/btn_bottom"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/btn_sort_h"
                        android:layout_marginTop="10sp"
                        android:background="@drawable/btn_bg"
                        android:text="@string/btn_bottom"
                        android:textColor="@color/white"
                        android:textSize="@dimen/historyscore_tb" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <!-- 保存名称及编码 -->

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="名称及编码:"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_12" />

                <EditText
                    android:id="@+id/tv_save_name"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/contact_icon_wh"
                    android:layout_margin="@dimen/text_size_6"
                    android:layout_weight="1"
                    android:background="@null"
                    android:drawableBottom="@drawable/rs_bottom_line"
                    android:gravity="center"
                    android:inputType="text"
                    android:text="药品采购"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_12" />

                <EditText
                    android:id="@+id/tv_save_code"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/contact_icon_wh"
                    android:layout_margin="@dimen/text_size_6"
                    android:layout_weight="1"
                    android:background="@null"
                    android:drawableBottom="@drawable/rs_bottom_line"
                    android:gravity="center"
                    android:inputType="text"
                    android:text="CO2"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_12" />

                <!-- 保存按钮 -->

                <Button
                    android:id="@+id/btn_save"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/btn_sort_h"
                    android:layout_gravity="center"
                    android:background="@drawable/btn_bg"
                    android:padding="3dp"
                    android:text="@string/btn_save"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_size_12" />
            </LinearLayout>

            <!-- 保存名称及编码 -->

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="名称及编码:"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_12" />

                <EditText
                    android:id="@+id/tv_add_name"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/contact_icon_wh"
                    android:layout_margin="@dimen/text_size_6"
                    android:layout_weight="1"
                    android:background="@null"
                    android:drawableBottom="@drawable/rs_bottom_line"
                    android:gravity="center"
                    android:inputType="text"
                    android:text="药品采购"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_12" />

                <EditText
                    android:id="@+id/tv_add_code"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/contact_icon_wh"
                    android:layout_margin="@dimen/text_size_6"
                    android:layout_weight="1"
                    android:background="@null"
                    android:drawableBottom="@drawable/rs_bottom_line"
                    android:gravity="center"
                    android:inputType="text"
                    android:text="CO2"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_12" />

                <!-- 新增按钮 -->

                <Button
                    android:id="@+id/btn_add"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/btn_sort_h"
                    android:layout_gravity="center"
                    android:background="@drawable/btn_bg"
                    android:padding="3dp"
                    android:text="@string/btn_add"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_size_12" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="vertical">

            <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="@color/shallowblue" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="8dp"
                android:gravity="center"
                android:orientation="horizontal">

                <!-- 提交按钮 -->

                <Button
                    android:id="@+id/changepsw_submite"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/contact_icon_wh"
                    android:background="@drawable/change_bg"
                    android:padding="6dp"
                    android:text="@string/submite"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_size_14" />

                <!-- 重置按钮 -->

                <Button
                    android:id="@+id/changepsw_reset"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/contact_icon_wh"
                    android:layout_marginLeft="10dp"
                    android:background="@drawable/change_bg"
                    android:padding="6dp"
                    android:text="@string/reset"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_size_14" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

</LinearLayout>